{
  "name": "pitchconnect",
  "version": "2.0.0",
  "description": "PitchConnect - The world's best multi-sport team management platform with real-time analytics, player tracking, injury management, tactical analysis, and AI-powered insights. Supporting Football, Netball, Rugby, Cricket, American Football, and Basketball.",
  "private": true,
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "Ange Bledoun",
    "email": "ange@getpitchconnect.com",
    "url": "https://getpitchconnect.com"
  },
  "homepage": "https://getpitchconnect.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ange-bledoun-pitchconnect/PitchConnect.git",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/ange-bledoun-pitchconnect/PitchConnect/issues",
    "email": "support@getpitchconnect.com"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@10.8.0",
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "dev:debug": "next dev --inspect --port 3000",
    "build": "npm run prisma:generate && next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit --skipLibCheck",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio --port 5555",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:validate": "prisma validate",
    "prisma:format": "prisma format",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run type-check && npm run lint && npm run test:coverage && npm run test:e2e",
    "test:smoke": "playwright test --grep @smoke",
    "test:redis": "node scripts/test-redis.js",
    "test:stripe": "node scripts/test-stripe.js",
    "test:email": "node scripts/test-email.js",
    "coverage:report": "vitest run --coverage --reporter=lcov",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "next-bundle-analyzer",
    "verify": "npm run type-check && npm run lint && npm run test",
    "verify:all": "npm run verify && npm run test:coverage && npm run test:e2e",
    "deploy:staging": "vercel deploy --env DATABASE_URL=$STAGING_DATABASE_URL",
    "deploy:production": "vercel deploy --prod --env DATABASE_URL=$PROD_DATABASE_URL",
    "docker:build": "docker build -t ghcr.io/pitchconnect/app:latest .",
    "docker:push": "docker push ghcr.io/pitchconnect/app:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f",
    "redis:start": "redis-server",
    "redis:cli": "redis-cli",
    "redis:test": "npm run test:redis",
    "redis:stop": "redis-cli SHUTDOWN",
    "clean": "rimraf .next node_modules dist coverage .turbo .eslintcache",
    "clean:cache": "rimraf .next .turbo .eslintcache",
    "clean:all": "npm run clean && npm install",
    "reinstall": "npm run clean:all",
    "prepare": "husky || true",
    "precommit": "lint-staged",
    "audit:security": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix --audit-level=moderate",
    "outdated": "npm outdated",
    "update:all": "npm update",
    "update:check": "npm outdated",
    "health-check": "npm run type-check && npm run lint",
    "setup:fresh": "npm install && npx prisma generate && npm run build",
    "env:validate": "node -e \"require('dotenv').config(); console.log('âœ“ Environment validated')\"",
    "db:health": "npx prisma db execute --stdin <<< \"SELECT 1\"",
    "cache:clear": "rimraf .next .turbo node_modules/.cache",
    "optimize": "npm run build && npm run analyze"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.7.4",
