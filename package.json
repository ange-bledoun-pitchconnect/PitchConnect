{
  "type": "module",
  "name": "pitchconnect",
  "version": "1.0.0",
  "description": "PitchConnect - The world's best multi-sport team management platform with real-time analytics, player tracking, and league management",
  "private": true,
  "license": "MIT",
  "author": {
    "name": "PitchConnect Team",
    "email": "support@pitchconnect.io"
  },
  "homepage": "https://pitchconnect.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/pitchconnect/pitchconnect.git"
  },
  "bugs": {
    "url": "https://github.com/pitchconnect/pitchconnect/issues",
    "email": "support@pitchconnect.io"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "npm run prisma:generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "eslint . --fix --ext .ts,.tsx --ignore-path .gitignore",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" --ignore-path .gitignore",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:pull": "prisma db pull",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:prod": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "prisma:seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "prisma:reset": "prisma migrate reset",
    "prisma:validate": "prisma validate",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:unit": "jest --testPathPattern=\"/tests/unit/\" --passWithNoTests",
    "test:integration": "jest --testPathPattern=\"/tests/integration/\" --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --passWithNoTests",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run type-check && npm run lint && npm run test:coverage && npm run test:e2e",
    "test:smoke": "playwright test --grep @smoke",
    "coverage:report": "jest --coverage --coverageReporters=lcov --passWithNoTests",
    "analyze": "ANALYZE=true npm run build",
    "analyze:bundle": "next-bundle-analyzer",
    "deploy:staging": "vercel deploy --env DATABASE_URL=$STAGING_DATABASE_URL",
    "deploy:production": "vercel deploy --prod --env DATABASE_URL=$PROD_DATABASE_URL",
    "docker:build": "docker build -t ghcr.io/pitchconnect/app:latest .",
    "docker:push": "docker push ghcr.io/pitchconnect/app:latest",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "clean": "rm -rf .next node_modules dist coverage .turbo",
    "clean:cache": "rm -rf .next .turbo",
    "clean:all": "npm run clean && npm install",
    "pre-commit": "lint-staged",
    "prepare": "husky install || true",
    "audit:security": "npm audit --audit-level=moderate",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@datadog/browser-rum": "^6.24.1",
    "@hookform/resolvers": "^3.10.0",
    "@mux/mux-node": "^12.8.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-accordion": "^1.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.3",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.3",
    "@radix-ui/react-progress": "^1.1.1",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.1.5",
    "@sentry/react": "^10.29.0",
    "@supabase/supabase-js": "^2.45.6",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@tanstack/react-query": "^5.90.12",
    "@upstash/redis": "^1.35.7",
    "@vercel/analytics": "^1.4.1",
    "@vercel/speed-insights": "^1.1.0",
    "axios": "^1.13.2",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.4",
    "csv-stringify": "^6.6.0",
    "dashjs": "^4.7.4",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.12.0",
    "hls.js": "^1.6.15",
    "html2canvas": "^1.4.1",
    "js-cookie": "^3.0.5",
    "jspdf": "^3.0.4",
    "lucide-react": "^0.462.0",
    "next": "^15.5.6",
    "next-auth": "^4.24.13",
    "next-bundle-analyzer": "^0.6.8",
    "next-themes": "^0.4.4",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.2",
    "qrcode": "^1.5.4",
    "react": "^19.0.0-rc.1",
    "react-day-picker": "^9.3.2",
    "react-dom": "^19.0.0-rc.1",
    "react-hook-form": "^7.68.0",
    "react-hot-toast": "^2.4.1",
    "recharts": "^2.15.4",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "sonner": "^2.0.7",
    "speakeasy": "^2.0.0",
    "stripe": "^17.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.31.0",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^9.0.0",
    "vaul": "^1.1.1",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.15.0",
    "@next/bundle-analyzer": "^15.5.7",
    "@playwright/test": "^1.57.0",
    "@sentry/nextjs": "^10.30.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.11",
    "@types/js-cookie": "^3.0.6",
